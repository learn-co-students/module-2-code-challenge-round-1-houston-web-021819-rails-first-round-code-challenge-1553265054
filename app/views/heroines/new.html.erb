<h1>Create a new super-heroine!</h1>

<%= form_with(model: @heroine) do |f|%>

  <%= f.label :name%>
  <%= f.text_field :name%><br><br>

  <%= f.label :super_name%>
  <%= f.text_field :super_name%><br><br>

  <%= f.label :power%>
  <%= f.collection_select :power_id, @powers, :id, :name %><br><br>

  <%= f.submit %>
  <%end%>