<%=form_for @heroine do |f|%>
<% if @heroine.errors.any? %>
    <%= @heroine.errors.full_messages %>
<%end%>
    <p>Name: <%=f.text_field :name%></p>
    <p>Super Name: <%=f.text_field :super_name%></p>
    <p>Super Power: <%=f.collection_select :power_id, Power.all, :id, :name%>
    <%=f.submit %>

<%end%>