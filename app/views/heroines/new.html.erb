<h2>Create a new heroine</h2>
<%= form_for(@heroine) do |f|%>
    
    <label>Enter the name:</label>
    <%= f.text_field :name %><br>

    <label>Enter the super woman name:</label>
    <%= f.text_field :super_name %><br>

    <label>Choose the super power:</label>
    <%= f.collection_select :power_id, Power.all, :id, :name %>

    <%= f.submit %>
<% end%>
